   "source": [
    "# 5.2 翻转\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "46db5d56",
   "metadata": {},
   "outputs": [],
   "source": [
    "import cv2\n",
    "\n",
    "img=cv2.imread(\"../images/lena.bmp\")\n",
    "\n",
    "x=cv2.flip(img,0)\n",
    "\n",
    "y=cv2.flip(img,1)\n",
    "\n",
    "xy=cv2.flip(img,-1)\n",
    "\n",
    "cv2.imshow(\"img\",img)\n",
    "\n",
    "cv2.imshow(\"x\",x)\n",
    "\n",
    "cv2.imshow(\"y\",y)\n",
    "\n",
    "cv2.imshow(\"xy\",xy)\n",
    "\n",
    "cv2.waitKey()\n",
    "\n",
    "cv2.destroyAllWindows()"
   ]
